{
  "name": "ngx-image-compress-demo",
  "version": "13.1.5",
  "author": "David Faure",
  "license": "MIT",
  "repository": "https://github.com/dfa1234/ngx-image-compress",
  "keywords": [
    "angular",
    "image",
    "upload",
    "compression"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "ci:build:app": "ng build ngx-image-compress-demo",
    "ci:build:library": "ng build ngx-image-compress",
    "ci:test:app": "ng test ngx-image-compress-demo --browsers=ChromeHeadless --watch=false --code-coverage",
    "ci:test:library": "ng test ngx-image-compress --browsers=ChromeHeadless --watch=false --code-coverage",
    "lib:generate": "ng generate library ngx-image-compress-new",
    "lib:watch": "ng build ngx-image-compress --watch",
    "lib:post-build": "cp ./README.md ./dist/ngx-image-compress/README.md && cp ./LICENSE ./dist/ngx-image-compress/LICENSE",
    "lib:build": "ng build ngx-image-compress && npm run lib:post-build",
    "lib:publish": "cd ./dist/ngx-image-compress && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "firebase": "^9.6.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.1.1",
    "typescript": "~4.5.2"
  }
}
